{{>header}}
<!-- Header // -->
<div class="container-fluid d-flex align-items-center page-name background-black mb-5">
    <div class="container text-white">
        <h1>{{page.title}}</h1>
    </div>
</div>

<!-- Content // -->
<div class="container">
    <div class="row distributors">
        {{#each partners}}
        <div class="col-md-4 my-5">
            <h3>
                {{title}} <br>
                <i>{{company}}</i>
            </h3>

            <p>
                {{{content}}}

            </p>
        </div>
        {{/each}}
    </div>
</div>
<!-- Content // -->
{{>footer}}